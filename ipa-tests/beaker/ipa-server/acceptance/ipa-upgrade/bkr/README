# MYNEWREPO* params can be added to ipa-upgrade to set the target repos

<task name="/CoreOS/ipa-server/rhel70-ipa/acceptance/ipa-upgrade" role="MASTER">
    <params>
        <param name="MYNEWREPO1" value="http://download.devel.redhat.com/nightly/latest-RHEL6/6/Server/x86_64/os/"/>
        <param name="MYNEWREPO2" value="http://download.devel.redhat.com/nightly/latest-RHEL6/6/Server/optional/x86_64/os/"/>
        <param name="MYNEWREPO3" value="http://cosmos.lab.eng.pnq.redhat.com/ipa-latest/x86_64/"/>
    </params>
</task>

