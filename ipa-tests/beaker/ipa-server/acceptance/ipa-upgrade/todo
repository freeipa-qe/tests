OSNAME=rhel
OSVER=65

getrepo()
{
    case $1 in
    63)
        myrepo1=
        myrepo2=
        myrepo3=
        ;;
    64)
        myrepo1=
        myrepo2=
        myrepo3=
        ;;
    65)
        myrepo1=
        myrepo2=
        myrepo3=
        ;;
    66)
        myrepo1=
        myrepo2=
        myrepo3=
        ;;
    67)
        myrepo1=
        myrepo2=
        myrepo3=
        ;;
    68)
        myrepo1=
        myrepo2=
        myrepo3=
        ;;
    69)
        myrepo1=
        myrepo2=
        myrepo3=
        ;;
    *)
        myrepo1=""
        myrepo2=""
        myrepo3=""
        break
        ;;
    esac
}

upgrade 63
upgrade 64
upgrade 69

OSVER=62
UPVER=65

CURVER=62
UPVER=$(( CURVER += 1 ))
TGTVER=69
for (( i=$UPVER ; i<=TGTVER ; i++ )); do 
    upgrade $i
done

upgrade()
{
    getrepo $1
    setrepo
    upgrade_master
    upgrade_replica
    upgrade_client
}
