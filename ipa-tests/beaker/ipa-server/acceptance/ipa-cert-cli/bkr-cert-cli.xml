<job>
	<whiteboard>
		freeipa CLI session support
	</whiteboard>
	<recipeSet>
		<recipe whiteboard="ipa dns tests - x86_64" ks_meta="selinux=disabled partitions=yes">
			<distroRequires>
				<and>
					<distro_family op="=" value="Fedora17"/>
					<distro_variant op="=" value="Fedora"/>
					<distro_arch op="=" value="x86_64"/>
				</and>
			</distroRequires>
			<hostRequires/>
			<repos>
				<repo name="jdennisipa" url="http://jdennis.fedorapeople.org/ipa-devel/fedora/17/x86_64/os/"/>
				<repo name="jdennisipadebug" url="http://jdennis.fedorapeople.org/ipa-devel/fedora/17/x86_64/debug/"/>
				<repo name="389-ds-updated" url="http://alpha.dsdev.sjc.redhat.com/export/home/mgregg/389-ds-bas-1.2.11.9-1/"/>
				<repo name="fc17-mv-updates" url="http://alpha.dsdev.sjc.redhat.com/export/archives/rhel/fc17-updates/x86_64/"/>
			</repos>
			<task name="/distribution/install" role="STANDALONE">
				<params/>
			</task>
			<task name="/distribution/pkginstall" role="STANDALONE">
				<params>
					<param name="PKGARGNAME" value="expect"/>
				</params>
			</task>
                        <task name="/CoreOS/ipa-server/shared" role="STANDALONE">
                               <params/>
                        </task>
			<task name="/CoreOS/ipa-server/acceptance/quickinstall" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-cert-cli" role="MASTER">
				<params/>
			</task>
		</recipe>
	</recipeSet>
</job>

