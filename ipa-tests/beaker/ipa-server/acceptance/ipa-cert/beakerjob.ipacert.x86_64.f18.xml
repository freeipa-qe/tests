<job>
	<whiteboard>
		ipa-server - Certificate CLI Tests x86_64 [#serial]
	</whiteboard>
	<notify/>
	<recipeSet>
		<recipe whiteboard="ipa server - Certificate CLI Tests">
		    <repos/>
            <repos>
                <repo name="ipa-extras" url="http://mickey.dsdev.sjc.redhat.com/repos/pki/ipa/devel/fedora-18/extras/x86_64/"/>
                <repo name="ipa-noarch" url="http://mickey.dsdev.sjc.redhat.com/repos/pki/ipa/devel/rhel6/devel_x86_64/noarch/"/>
            </repos>
            <distroRequires>
                <and>
					<distro_family op="=" value="Fedora18"/>
					<distro_variant op="=" value="Fedora"/>
					<distro_name op="=" value="Fedora-18"/>
					<distro_arch op="=" value="x86_64"/>
                </and>
                <distro_virt op="=" value=""/>
            </distroRequires>
			<hostRequires/>
			<task name="/distribution/install" role="STANDALONE">
				<params/>
			</task>
			<task name="/distribution/pkginstall" role="STANDALONE">
				<params>
				<param name="PKGARGNAME" value="expect"/>
				</params>
			</task>
            <task name="/CoreOS/ipa-server/rhel70-ipa/shared" role="STANDALONE">
                <params/>
            </task>
			<task name="/CoreOS/ipa-server/rhel70-ipa/acceptance/quickinstall" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/rhel70-ipa/acceptance/ipa-cert" role="MASTER">
				<params/>
			</task>
			
		</recipe>
	</recipeSet>
</job>

