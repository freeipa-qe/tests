<job>
	<whiteboard>
		IPA CLI session support x86_64 [#serial]
	</whiteboard>
	<recipeSet>
		<recipe whiteboard="ipa client session tests - x86_64" role="MASTER" ks_meta="selinux=disabled partitions=yes">
			<distroRequires>
				<and>
                    <distro_family op="=" value="RedHatEnterpriseLinux6"/>
                    <distro_variant op="=" value="Server"/>
                    <distro_arch op="=" value="x86_64"/>
				</and>
			</distroRequires>
			<hostRequires/>
			<repos/>
			<task name="/distribution/install" role="STANDALONE">
				<params/>
			</task>
			<task name="/distribution/pkginstall" role="STANDALONE">
				<params>
					<param name="PKGARGNAME" value="expect"/>
				</params>
			</task>
                <task name="/CoreOS/ipa-server/rhel70-ipa/shared" role="STANDALONE">
                    <params/>
                </task>
			<task name="/CoreOS/ipa-server/rhel70-ipa/acceptance/quickinstall" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/rhel70-ipa/acceptance/ipa-sessions-cli" role="MASTER">
				<params/>
			</task>
		</recipe>
	</recipeSet>
</job>

