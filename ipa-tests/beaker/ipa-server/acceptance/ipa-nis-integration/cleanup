for user in $(ipa user-find|grep "User login:"|awk '{print $3}'|grep -v admin); do
	ipa user-del $user
done

for group in $(ipa group-find|grep "Group name:"|awk '{print $3}'|egrep -v "ipausers|admins|editors"); do
	ipa group-del $group
done

for host in $(ipa host-find|grep "Host name:"|awk '{print $3}'|egrep -v "$MASTER"); do
	ipa host-del $host --updatedns
done

for netgroup in $(ipa netgroup-find|grep "Netgroup name:"|awk '{print $3}'); do
	ipa netgroup-del $netgroup
done

for dnszone in $(ipa dnszone-find|grep "Zone name:"|awk '{print $3}'|egrep -v "testrelm.com|122.168.192.in-addr.arpa."); do
	ipa dnszone-del $dnszone
done

