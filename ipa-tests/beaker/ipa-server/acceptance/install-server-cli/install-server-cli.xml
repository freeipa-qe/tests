<job retention_tag="scratch">
	<whiteboard>
		ipa-server install cli tests fedora 17 x86_64
	</whiteboard>
	<recipeSet priority="Normal">
		<recipe kernel_options="" kernel_options_post="" ks_meta="selinux=disabled partitions=yes" role="None" whiteboard="ipa server-install cli - fedora x86_64">
			<autopick random="false"/>
			<watchdog panic="None"/>
			<packages/>
			<ks_appends/>
			<repos>
				<repo name="ipa" url="http://jdennis.fedorapeople.org/ipa-devel/fedora/17/i386/os/"/>
				<repo name="fc17-mv-updates" url="http://alpha.dsdev.sjc.redhat.com/export/archives/rhel/fc17-updates/i386/"/>
			</repos>
			<distroRequires>
				<and>
					<distro_family op="=" value="Fedora17"/>
					<distro_virt op="=" value=""/>
					<distro_variant op="=" value="Fedora"/>
					<distro_name op="=" value="Fedora-17"/>
					<distro_arch op="=" value="i386"/>
				</and>
			</distroRequires>
			<hostRequires>
				<system_type value="Machine"/>
				<memory op="&gt;=" value="1900"/>
			</hostRequires>
			<partitions/>
			<task name="/distribution/install" role="STANDALONE">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/shared" role="STANDALONE">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/quickinstall" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/install-server-cli" role="MASTER">
				<params/>
			</task>
		</recipe>
	</recipeSet>
</job>

