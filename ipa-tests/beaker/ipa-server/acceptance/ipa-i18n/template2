rlPhaseStartTest "ipa-i18n-%num: Changing firstname of $uname2 to %char"
    rlRun "ipa user-mod --first='%char' $uname2" 0 "Modifying firstname of $uname2 to %char"
rlPhaseEnd

rlPhaseStartTest "ipa-i18n-%bnum: checking to ensuer that the firstname of $uname2 is %char"
        rlRun "ipa user-find --all $uname2 | grep First | grep '%char'" 0 "Checking to ensure that the firstname of $uname2 is %char"
rlPhaseEnd

