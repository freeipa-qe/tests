rlPhaseStartTest "ipa-i18n-%num: Changing lastname of $uname1 to %char"
    rlRun "ipa user-mod --last='%char' $uname1" 0 "Modifying lastname of $uname1 to %char"
rlPhaseEnd

rlPhaseStartTest "ipa-i18n-%bnum: checking to ensuer that the lastname of $uname1 is %char"
        rlRun "ipa user-find --all $uname1 | grep '%char'" 0 "Checking to ensure that the lastname of $uname1 is %char"
rlPhaseEnd

