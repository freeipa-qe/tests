<job>
	<whiteboard>
		ipa-server - Password CLI Tests
	</whiteboard>
	<notify>
        <cc>yzhang@redhat.com</cc>
    </notify>
	<recipeSet>
		<recipe whiteboard="ipa server - Password CLI Tests">
			<distroRequires>
				<and>
					<distro_family op="=" value="Fedora14"/>
					<distro_method op="=" value="nfs"/>
					<distro_tag op="=" value="STABLE"/>
					<distro_arch op="=" value="x86_64"/>
				</and>
			</distroRequires>
			<hostRequires>
                <and>
                    <hostname op="=" value="mv32b-vm.idm.lab.bos.redhat.com" />
                </and>
			</hostRequires>
			<repos/>
			<task name="/distribution/install" role="STANDALONE">
				<params/>
			</task>
			<task name="/distribution/pkginstall" role="STANDALONE">
				<params>
				<param name="PKGARGNAME" value="expect"/>
				</params>
			</task>
            <task name="/CoreOS/ipa-server/rhel70-ipa/shared" role="STANDALONE">
                <params/>
            </task>

			<task name="/CoreOS/ipa-server/rhel70-ipa/acceptance/install" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/rhel70-ipa/acceptance/ipa-password" role="MASTER">
				<params>
					<param name="KILLTIMEOVERRIDE" value="8789333"/>
				</params>
			</task>
			<task name="/CoreOS/ipa-server/rhel70-ipa/acceptance/uninstall" role="STANDALONE">
				<params/>
			</task>

		</recipe>
	</recipeSet>
</job>

