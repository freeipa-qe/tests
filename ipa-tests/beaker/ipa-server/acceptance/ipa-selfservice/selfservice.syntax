[selfservice-show]
# --all       Retrieve and print all attributes from the server. Affects command output.
# --raw       Print entries as stored on the server. Only affects output format.
--all: all
--raw: all

[selfservice-add]
# --permissions=LIST  Comma-separated list of permissions to grant (read, write). Default is write.
# --all               Retrieve and print all attributes from the server. Affects command output.
# --attrs=LIST        Comma-separated list of attributes
# --raw               Print entries as stored on the server. Only affects output format.
--permissions: all
--all: all
--attrs: all
--raw: all

[selfservice-mod]
# --permissions=LIST  Comma-separated list of permissions to grant (read, write). Default is write.
# --all               Retrieve and print all attributes from the server. Affects command output.
# --attrs=LIST        Comma-separated list of attributes
# --raw               Print entries as stored on the server. Only affects output format.
--permissions: all
--all: all
--attrs: all
--raw: all

[selfservice-del]
# 
: all

[selfservice-find]
# --permissions=LIST  Comma-separated list of permissions to grant (read, write). Default is write.
# --name=STR          Self-service name
# --all               Retrieve and print all attributes from the server. Affects command output.
# --attrs=LIST        Comma-separated list of attributes
# --raw               Print entries as stored on the server. Only affects output format.
--permissions: all
--name: all
--all: all
--attrs: all
--raw: all

