[selfservice-show]
# --all       Retrieve and print all attributes from the server. Affects command output.
--all : none

# --raw       Print entries as stored on the server. Only affects output format.
--raw : none


[selfservice-add]
# --permissions=LIST  Comma-separated list of permissions to grant (read, write). Default is write.
--permissions (positive):LIST
--permissions (negative):LIST

# --all               Retrieve and print all attributes from the server. Affects command output.
--all : none

# --attrs=LIST        Comma-separated list of attributes
--attrs (positive):LIST
--attrs (negative):LIST

# --raw               Print entries as stored on the server. Only affects output format.
--raw : none


[selfservice-mod]
# --permissions=LIST  Comma-separated list of permissions to grant (read, write). Default is write.
--permissions (positive):LIST
--permissions (negative):LIST

# --all               Retrieve and print all attributes from the server. Affects command output.
--all : none

# --attrs=LIST        Comma-separated list of attributes
--attrs (positive):LIST
--attrs (negative):LIST

# --raw               Print entries as stored on the server. Only affects output format.
--raw : none


[selfservice-del]
# 
 (positive):none
 (negative):none


[selfservice-find]
# --permissions=LIST  Comma-separated list of permissions to grant (read, write). Default is write.
--permissions (positive):LIST
--permissions (negative):LIST

# --name=STR          Self-service name
--name (positive):STR
--name (negative):STR

# --all               Retrieve and print all attributes from the server. Affects command output.
--all : none

# --attrs=LIST        Comma-separated list of attributes
--attrs (positive):LIST
--attrs (negative):LIST

# --raw               Print entries as stored on the server. Only affects output format.
--raw : none


