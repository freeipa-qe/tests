<job>
	<notify>
		<cc>
		mgregg@redhat.com
		</cc> 
		<cc>
		ckannan@redhat.com
		</cc> 
		<cc>
		yzhang@redhat.com
		</cc> 
		<cc>
		jgalipea@redhat.com
		</cc> 
	</notify>
	<whiteboard>
		ipa repoclone Fedora14 x86_64
	</whiteboard>
	<recipeSet>
		<recipe whiteboard="">
			<distroRequires>
				<and>
					<distro_family op="=" value="Fedora14"/>
					<distro_method op="=" value="nfs"/>
					<distro_tag op="=" value="STABLE"/>
					<distro_arch op="=" value="x86_64"/>
				</and>
			</distroRequires>
			<repos>
				<repo name="ipa" url="http://jdennis.fedorapeople.org/ipa-devel/fedora/14/x86_64/os/"/>
				<repo name="beaker-harness" url="http://beaker-stage.app.eng.bos.redhat.com/harness/Fedora14/\$basearch"/>
				<repo name="beaker-redhat" url="http://apoc.dsdev.sjc.redhat.com/tet/beakerlib-fc14/"/>
			</repos>
			<hostRequires>
                                <and>
                                <hostname op="=" value="qe-blade-12.idm.lab.bos.redhat.com"/>
 
				</and>
			</hostRequires>
			<task name="/distribution/install" role="STANDALONE">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/repoclone" role="MASTER">
				<params/>
			</task>

		</recipe>
	</recipeSet>
</job>

