<job>
	<notify>
		<cc>ipa-and-samba-team-automation@redhat.com</cc>
	</notify>
	<whiteboard>
		ipa-server full acceptance - i386 - master only
	</whiteboard>
	<recipeSet>
		<recipe whiteboard="ipa server full acceptance - i386">
			<distroRequires>
				<and>
					<distro_variant op="=" value="Server"/>
					<distro_arch op="=" value="i386"/>	
				        <distro_tag op="=" value="STABLE"/>
				        <distro_family op="=" value="RedHatEnterpriseLinux6"/>
				        <distro_method op="=" value="nfs"/>
				</and>
			</distroRequires>
			<hostRequires/>
			<repos>
				<repo name="ipa" url="http://mickey.dsdev.sjc.redhat.com/repos/pki/ipa/devel/rhel6/devel_i686/i686/"/>
				<repo name="ipa_noarch" url="http://mickey.dsdev.sjc.redhat.com/repos/pki/ipa/devel/rhel6/devel_i686/noarch/"/>
			</repos>
			<task name="/distribution/install" role="STANDALONE">
				<params/>
			</task>
			<task name="/distribution/pkginstall" role="STANDALONE">
				<params>
					<param name="PKGARGNAME" value="expect"/>
				</params>
			</task>
                        <task name="/CoreOS/ipa-server/shared" role="STANDALONE">
                               <params/>
                        </task>
			<task name="/CoreOS/ipa-server/acceptance/quickinstall" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-user-cli/adduser" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-user-cli/moduser" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-group-cli" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-host-cli" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-hostgroup-cli" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-netgroup-cli" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-hbac-cli" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-hbacsvc-cli" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-get-rm-keytab" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-krbtpolicy" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-config" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-dns" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-i18n" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-ctl" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/uninstall" role="STANDALONE">
				<params/>
			</task>
		</recipe>
	</recipeSet>
</job>

