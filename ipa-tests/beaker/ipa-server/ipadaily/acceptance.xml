<job>
	<notify>
		<cc>mgregg@redhat.com</cc>
		<cc>jgalipea@redhat.com</cc>
		<cc>yzhang@redhat.com</cc>
		<cc>ipa-and-samba-team-list@redhat.com</cc>
	</notify>
	<whiteboard>
		ipa-server full run on qe-blade-12 x86_64
	</whiteboard>
	<recipeSet>
		<recipe whiteboard="ipa server on x86_64 on qe-blade-12">
			<distroRequires>
				<and>
					<distro_family op="=" value="Fedora14"/>
					<distro_method op="=" value="nfs"/>
					<distro_tag op="=" value="STABLE"/>
					<distro_arch op="=" value="x86_64"/>
				</and>
			</distroRequires>
			<hostRequires>
				<and/>
			</hostRequires>
			<repos>
			 <repo name="ipa" url="http://jdennis.fedorapeople.org/ipa-devel/fedora/14/x86_64/os/"/>
			 <repo name="beaker-harness" url="http://beaker-stage.app.eng.bos.redhat.com/harness/Fedora14/\$basearch"/>
			</repos>
			<task name="/distribution/install" role="STANDALONE">
				<params/>
			</task>
			<task name="/distribution/pkginstall" role="STANDALONE">
				<params>
					<param name="PKGARGNAME" value="expect"/>
				</params>
			</task>
                        <task name="/CoreOS/ipa-server/shared" role="STANDALONE">
                               <params/>
                        </task>

			<task name="/CoreOS/ipa-server/acceptance/install" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-user-cli/adduser" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-user-cli/moduser" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-group-cli" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-host-cli" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-hostgroup-cli" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-netgroup-cli" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-hbac-cli" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/ipa-hbacsvc-cli" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/nis-cli" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/uninstall" role="STANDALONE">
				<params/>
			</task>

		</recipe>
	</recipeSet>
</job>

