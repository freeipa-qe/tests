[beakerharness]
name=beaker-harness
baseurl=https://beaker-devel.app.eng.bos.redhat.com/harness/RedHatEnterpriseLinux6/$basearch/
enabled=1
metadata_expire=7d
gpgcheck=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-$basearch
sslverify=0

[beakerharness2]
name=beaker-harness2
baseurl=https://beaker-devel.app.eng.bos.redhat.com/harness/epel-6/$basearch/
enabled=1
metadata_expire=7d
gpgcheck=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-$basearch
sslverify=0

[epel]
name=Extra Packages for Enterprise Linux 6 - $basearch
#baseurl=http://download.fedoraproject.org/pub/epel/6/$basearch
mirrorlist=http://mirrors.fedoraproject.org/mirrorlist?repo=epel-6&arch=$basearch
failovermethod=priority
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL
