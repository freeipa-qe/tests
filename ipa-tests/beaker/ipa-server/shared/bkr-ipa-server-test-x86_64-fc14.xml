<job>
	<whiteboard>
		ipa-server x86_64
	</whiteboard>
	<recipeSet>
		<recipe whiteboard="">
			<distroRequires>
				<and>
					<distro_family op="=" value="Fedora14"/>
					<distro_method op="=" value="nfs"/>
					<distro_tag op="=" value="STABLE"/>
					<distro_arch op="=" value="x86_64"/>
				</and>
			</distroRequires>
			<repos>
				<repo name="ipa" url="http://jdennis.fedorapeople.org/ipa-devel/fedora/14/x86_64/os/"/>
				<repo name="beaker-harness" url="http://beaker-stage.app.eng.bos.redhat.com/harness/Fedora14/x86_64"/>
				<repo name="beakerlib-fedora" url="http://apoc.dsdev.sjc.redhat.com/tet/beakerlib-fc14/"/>
			</repos>
			<hostRequires>
				<and/>
			</hostRequires>
			<task name="/distribution/install" role="STANDALONE">
				<params/>
			</task>
			<task name="/distribution/pkginstall" role="STANDALONE">
				<params>
					<param name="PKGARGNAME" value="beakerlib-redhat"/>
				</params>
			</task>
			<task name="/CoreOS/ipa-server/shared" role="STANDALONE">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/install" role="MASTER">
				<params/>
			</task>
			<task name="/CoreOS/ipa-server/acceptance/uninstall" role="STANDALONE">
				<params/>
			</task>

		</recipe>
	</recipeSet>
</job>

