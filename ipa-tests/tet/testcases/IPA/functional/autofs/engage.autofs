# This function will set the default values for the variables needed.
#
autofs_default()
{
	if [ -z "$autofsRunIt" ]
	then
		autofsRunIt=n
	fi
}

# This function will ask the user for more information/choices if needed.
#
autofs_ask()
{

	sav_autofsRunIt=$autofsRunIt
	echo "    Execute autofs test suite [$autofsRunIt] ? \c"
	read rsp
	case $rsp in
		"")	autofsRunIt=$sav_autofsRunIt	;;
		y|Y)	
			autofsRunIt=y
			;;
		*)	autofsRunIt=n		;;
	esac

}

# This function will print the user's choices (aka variables)
#
autofs_print()
{
	echo "    Execute autofs test suite        : $autofsRunIt"
}

# This function will echo in shell's format the user's choices
# It is the calling function that will redirect the output to
# the saved config file.
#
autofs_save()
{
	echo "autofsRunIt=$autofsRunIt"
}

# This function will check that the test suite may be executed
# It may also perform some kind of pre-configuration of the machine.
# This function should "exit 1" if there is problem.
#
autofs_check()
{
	kgb=kgb
}

# This function will startup/initiate the test suite
#
autofs_startup()
{
:
}

# This function will run the test suite
#
autofs_run()
{
	if [ $autofsRunIt = n ]
	then
		return
	fi
	echo "autofs run..."
	echo "$TET_ROOT/$MainTccName -e -s $TET_ROOT/testcases/IPA/functional/autofs/tet_scen -x $TET_ROOT/testcases/IPA/tetexecpl.cfg $TET_ROOT/testcases/IPA/functional/autofs autofs"

	(
	$TET_ROOT/$MainTccName \
		-e -s $TET_ROOT/testcases/IPA/functional/autofs/tet_scen \
		-x $TET_ROOT/testcases/IPA/tetexecpl.cfg \
		$TET_ROOT/testcases/IPA/functional/autofs\
		autofs > $MainTmpDir/autofs.run.out 2>&1
	)&
	EngageTimer $! 1200 120 # wait 1200 sec before kill, then 1200 until kill -9
	echo ""
	echo "autofs run $MainTmpDir/autofs.run.out"
	echo ""
	cat $MainTmpDir/autofs.run.out
	echo ""
	main_analyze "autofs run" `grep "tcc: journal file is" $MainTmpDir/autofs.run.out | awk '{print $5}'` $MainTmpDir/autofs.run.out
	MainReportFiles="$MainReportFiles $MainTmpDir/autofs.run.out"

	Gfile="$TET_TMP_DIR/global_src_`uname -n`"
	rm -f $Gfile
}

# This function will cleanup after the test suite execution
#
autofs_cleanup()
{
:
}


