# This function will set the default values for the variables needed.
#
kinit_default()
{
	if [ -z "$kinitRunIt" ]
	then
		kinitRunIt=n
	fi
}

# This function will ask the user for more information/choices if needed.
#
kinit_ask()
{

	sav_kinitRunIt=$kinitRunIt
	echo "    Execute kinit test suite [$kinitRunIt] ? \c"
	read rsp
	case $rsp in
		"")	kinitRunIt=$sav_kinitRunIt	;;
		y|Y)	
			kinitRunIt=y
			;;
		*)	kinitRunIt=n		;;
	esac

}

# This function will print the user's choices (aka variables)
#
kinit_print()
{
	echo "    Execute kinit test suite        : $kinitRunIt"
}

# This function will echo in shell's format the user's choices
# It is the calling function that will redirect the output to
# the saved config file.
#
kinit_save()
{
	echo "kinitRunIt=$kinitRunIt"
}

# This function will check that the test suite may be executed
# It may also perform some kind of pre-configuration of the machine.
# This function should "exit 1" if there is problem.
#
kinit_check()
{
	kgb=kgb
}

# This function will startup/initiate the test suite
#
kinit_startup()
{
:
}

# This function will run the test suite
#
kinit_run()
{
	if [ $kinitRunIt = n ]
	then
		return
	fi
	echo "kinit run..."
	echo "$TET_ROOT/$MainTccName -e -s $TET_ROOT/testcases/IPA/stress/kinit/tet_scen -x $TET_ROOT/testcases/IPA/tetexecpl.cfg $TET_ROOT/testcases/IPA/stress/kinit kinit"

	$TET_ROOT/$MainTccName \
		-e -s $TET_ROOT/testcases/IPA/stress/kinit/tet_scen \
		-x $TET_ROOT/testcases/IPA/tetexecpl.cfg \
		$TET_ROOT/testcases/IPA/stress/kinit \
		kinit > $MainTmpDir/kinit.run.out 2>&1
	echo ""
	echo "kinit run $MainTmpDir/kinit.run.out"
	echo ""
	cat $MainTmpDir/kinit.run.out
	echo ""
	main_analyze "kinit run" `grep "tcc: journal file is" $MainTmpDir/kinit.run.out | awk '{print $5}'` $MainTmpDir/kinit.run.out
	MainReportFiles="$MainReportFiles $MainTmpDir/kinit.run.out"

	Gfile="$TET_TMP_DIR/global_src_`uname -n`"
	rm -f $Gfile
}

# This function will cleanup after the test suite execution
#
kinit_cleanup()
{
:
}


