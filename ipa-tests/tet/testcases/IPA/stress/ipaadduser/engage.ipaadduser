# This function will set the default values for the variables needed.
#
ipaadduser_default()
{
	if [ -z "$ipaadduserRunIt" ]
	then
		ipaadduserRunIt=n
	fi
}

# This function will ask the user for more information/choices if needed.
#
ipaadduser_ask()
{

	sav_ipaadduserRunIt=$ipaadduserRunIt
	echo "    Execute ipaadduser test suite [$ipaadduserRunIt] ? \c"
	read rsp
	case $rsp in
		"")	ipaadduserRunIt=$sav_ipaadduserRunIt	;;
		y|Y)	
			ipaadduserRunIt=y
			;;
		*)	ipaadduserRunIt=n		;;
	esac

}

# This function will print the user's choices (aka variables)
#
ipaadduser_print()
{
	echo "    Execute ipaadduser test suite        : $ipaadduserRunIt"
}

# This function will echo in shell's format the user's choices
# It is the calling function that will redirect the output to
# the saved config file.
#
ipaadduser_save()
{
	echo "ipaadduserRunIt=$ipaadduserRunIt"
}

# This function will check that the test suite may be executed
# It may also perform some kind of pre-configuration of the machine.
# This function should "exit 1" if there is problem.
#
ipaadduser_check()
{
	kgb=kgb
}

# This function will startup/initiate the test suite
#
ipaadduser_startup()
{
:
}

# This function will run the test suite
#
ipaadduser_run()
{
	if [ $ipaadduserRunIt = n ]
	then
		return
	fi
	echo "ipaadduser run..."
	echo "$TET_ROOT/$MainTccName -e -s $TET_ROOT/testcases/IPA/stress/ipaadduser/tet_scen -x $TET_ROOT/testcases/IPA/tetexecpl.cfg $TET_ROOT/testcases/IPA/stress/ipaadduser ipaadduser"

	$TET_ROOT/$MainTccName \
		-e -s $TET_ROOT/testcases/IPA/stress/ipaadduser/tet_scen \
		-x $TET_ROOT/testcases/IPA/tetexecpl.cfg \
		$TET_ROOT/testcases/IPA/stress/ipaadduser \
		ipaadduser > $MainTmpDir/ipaadduser.run.out 2>&1
	echo ""
	echo "ipaadduser run $MainTmpDir/ipaadduser.run.out"
	echo ""
	cat $MainTmpDir/ipaadduser.run.out
	echo ""
	main_analyze "ipaadduser run" `grep "tcc: journal file is" $MainTmpDir/ipaadduser.run.out | awk '{print $5}'` $MainTmpDir/ipaadduser.run.out
	MainReportFiles="$MainReportFiles $MainTmpDir/ipaadduser.run.out"

	Gfile="$TET_TMP_DIR/global_src_`uname -n`"
	rm -f $Gfile
}

# This function will cleanup after the test suite execution
#
ipaadduser_cleanup()
{
:
}


