<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="IPAGroupTestSuite">	
	<test name="Group Tests">
		<packages>
		    <package name="com.redhat.qe.ipa.sahi.*" />
		</packages>
		<groups>
			<run>
			    <include name="setup" />
			    <include name="init" /> 
				<include name="addGroupTests" /> 
				<include name="deleteGroupTests" />
				<!--
				<include name="teardown"/>
				-->				
			</run>
		</groups>
		<classes>
		    <class name="com.redhat.qe.ipa.sahi.tests.group.GroupTests" />
		</classes>
	</test>
	
	<listeners>        
  		<listener class-name="com.redhat.qe.auto.testng.TestNGListener" />	
	</listeners>
	
</suite>