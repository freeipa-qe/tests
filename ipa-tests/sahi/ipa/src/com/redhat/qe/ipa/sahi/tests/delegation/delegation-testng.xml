<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="IPADelegationTestSuite">	
	<test name="Delegation Tests">
		<groups>
			<run> 
				<include name="add" />
				<include name="addLong" /> 
				<include name="addNegative" />
				<include name="addDuplicate" />
				<include name="modify" />
				<include name="modify with no delegation" />
				<include name="modify with delegation" />
				<include name="search" />
				<include name="searchNegative" />
				<include name="delete" />
				
				
			</run>
		</groups>
		<classes>
			<class name="com.redhat.qe.ipa.sahi.tests.delegation.DelegationTests" />
		</classes>
	</test> 
	<listeners>        
		<listener class-name="com.redhat.qe.auto.testng.TestNGListener" />	
	</listeners> 
</suite>
