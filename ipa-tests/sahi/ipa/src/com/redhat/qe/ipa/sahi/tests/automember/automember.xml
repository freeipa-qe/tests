<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="IPAAutomemberTestSuite">	
  <test name="IPAAutomember Bug Verification Tests">
            	<groups>
            	    <run>
            	    	<include name="setup" />
         				<include name="MissingSpecifiedName_Bug818258" />
         				<include name="MissingSpecifiedName_Bug818258_Cleanup" />
					</run>
            	</groups>
            	<classes>
            	    <class name="com.redhat.qe.ipa.sahi.tests.automember.Automember_Bug_Verification" />
            	</classes>
    </test>
  
  <test name="IPAAutomember Tests">
            	<groups>
            	    <run>
         				<include name="add" />
						<include name="modify" />
						<include name="delete" />
						<include name="search" />
						<include name="nonStandardAutomember" />
						
            	    </run>
            	</groups>
            	<classes>
            	    <class name="com.redhat.qe.ipa.sahi.tests.automember.AutomemberTests" />
            	    
            	</classes>
    </test> 
    
   
    <listeners>        
  	             <listener class-name="com.redhat.qe.auto.testng.TestNGListener" />	
    </listeners>
	
</suite>
