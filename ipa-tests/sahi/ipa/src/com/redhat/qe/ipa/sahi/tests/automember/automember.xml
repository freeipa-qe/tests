<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="IPAAutomemberTestSuite">	
    <test name="IPAAutomember Tests">
            	<groups>
            	    <run>
         				<include name="add" />
						<include name="modify" />
						<include name="delete" />
						<include name="search" />
						<include name="nonStandardAutomember" />
						<!-- <include name="report" />-->
            	    </run>
            	</groups>
            	<classes>
            	    <class name="com.redhat.qe.ipa.sahi.tests.automember.AutomemberTests" />
            	</classes>
    </test>
	
    <listeners>        
  	             <listener class-name="com.redhat.qe.auto.testng.TestNGListener" />	
    </listeners>
	
</suite>
