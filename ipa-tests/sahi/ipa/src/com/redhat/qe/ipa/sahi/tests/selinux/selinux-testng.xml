			    <include name="selinuxUserMapMemberCategory" />
			    <include name="selinuxUserMapNegativeSelinuxUser" />
			    <include name="selinuxUserMapModifyNegativeSelinuxUser" />
			</run>
		</groups>
	</test>
	
	<listeners>        
  		<listener class-name="com.redhat.qe.auto.testng.TestNGListener" />	
	</listeners>
	
</suite>