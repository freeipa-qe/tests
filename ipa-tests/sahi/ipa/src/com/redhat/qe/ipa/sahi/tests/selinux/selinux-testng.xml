<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="IPASelinuxUserMapTestSuite">	
	<test name="Selinux User Map Tests">
		<packages>
		    <package name="com.redhat.qe.ipa.sahi.*" />
		</packages>
		<groups>
			<run>
			    <include name="setup" />
			    <include name="addSelinuxUserMapTests" />
			    <include name="addAndAddAnotherSelinuxUserMapTests" />
			    <include name="addAndEditSelinuxUserMapTests" />
			    <include name="selinuxUserMapActionsTests" />
			    <include name="selinuxUserMapUndoRefreshResetUpdateTests" />
			    <include name="selinuxUserMapUpdateResetCancelTests" />
			    <include name="selinuxUserMapDeleteSingleTests" />
			    <include name="selinuxUserMapEnableDisableMultipleTests" />
			    <include name="selinuxUserMapDeleteMultipleTests" />
			    <include name="selinuxUserMapMemberTests" />
			    <include name="selinuxUserMapDisabledHBACRuleTests" />
			    <include name="addNegativeSelinuxUserMapTests" />
			    <include name="selinuxUserMapDeleteNegativeHBACRuleTests" />
			    <include name="selinuxUserMapMembersNegativeTests" />
			    <include name="selinuxUserMapHBACRuleNegativeTests" />
			    <include name="addSelinuxUserMapSyntaxTests" />
			    <include name="selinuxUserMapSearchTests" />
