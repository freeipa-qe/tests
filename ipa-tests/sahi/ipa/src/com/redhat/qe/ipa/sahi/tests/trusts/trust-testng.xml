<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="IPAServerTrustsTestSuite">	
	<test name="Trusts Tests">
		<packages>
		     <package name="com.redhat.qe.ipa.sahi.*" />
		</packages>
		<groups>
			<run>
			
			<include name="setup" />
			<include name="trustsAddTests" />
			<include name="duplicateTrustTests" />
			<include name="trustsAddAndCancelTests" />
			<include name="trustsAddNegativeTests" />
			<include name="trustsAddAndAnotherTests" />
			<include name="trustsAddAndEditTests" />
			<include name="trustsSettingTests" />
			<include name="trustsExpandCollapseTests" />
			<include name="preSharedPasswordTests" />
			<include name="trustsdeleteAndCancelTests" />
			<include name="trustsdeleteTests" />
			
			
			</run>
		</groups>
		<classes>
		    <class name="com.redhat.qe.ipa.sahi.tests.trusts.TrustsTests" />
		</classes>
	</test>
	
	<listeners>        
  		<listener class-name="com.redhat.qe.auto.testng.TestNGListener" />	
	</listeners>
	
</suite>