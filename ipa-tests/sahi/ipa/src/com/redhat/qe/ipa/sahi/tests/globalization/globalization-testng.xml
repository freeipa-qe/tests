<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="IPAGlobalizationTestSuite">	
	<test name="Globalization Tests">
		<groups>
			<run> 
				<include name="init" />
				<include name="add" /> 
				<include name="modify" />
				<include name="delete" />
				<include name="report" />
			</run>
		</groups>
		<classes>
			<class name="com.redhat.qe.ipa.sahi.tests.globalization.GlobalizationWebAutomation" />
		</classes>
	</test> 
	<listeners>        
		<listener class-name="com.redhat.qe.auto.testng.TestNGListener" />	
	</listeners> 
</suite>
